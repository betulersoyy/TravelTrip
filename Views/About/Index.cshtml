@using Travel_Trip_Proje.Models.Siniflar
@model List<Hakkımızda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var x in Model)
            {
                <img  src="@x.FotoUrl" alt="" style="max-width:1000px; min-width:1000px; max-height:450px; min-height:450px;"/>
                <p>@x.Aciklama</p>
            }

        </div>
    </div>
</div>

